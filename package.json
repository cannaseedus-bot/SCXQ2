{
  "name": "scxq2-cc",
  "version": "1.0.0",
  "description": "SCXQ2 Compression Calculus Engine - Deterministic, proof-generating, content-addressable compression for language packs",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "scxq2": "./dist/cli.mjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./verify": {
      "import": "./dist/verify.js",
      "default": "./dist/verify.js"
    },
    "./wasm-decoder": {
      "import": "./dist/wasm-decoder.js",
      "default": "./dist/wasm-decoder.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "cp src/*.js dist/",
    "test": "node --test test/*.test.mjs",
    "test:fuzz": "node --test test/fuzz_decode.test.mjs",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "compression",
    "calculus",
    "scxq2",
    "dictionary",
    "encoding",
    "deterministic",
    "content-addressable",
    "sha256",
    "proof",
    "language-pack",
    "asx",
    "kuhul",
    "cc-v1"
  ],
  "author": "ASX",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/asx/scxq2-cc.git"
  },
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false
}
